sayi = int(input("Bir sayi giriniz: "))

# Asal sayiliar 1 den büyüktür.
if sayi <= 1:   
      print("1 den kücük asal sayi yoktur,{} sayisi asal degildir".format(sayi))
else:
   prime= True
   for i in range(2,sayi):
      if (sayi % i == 0):
         prime= False   

               
   if prime == True:
      print(sayi,"Sayisi asaldir.")
   else:
      print(sayi,"Sayisi asal degildir.")
   
     









